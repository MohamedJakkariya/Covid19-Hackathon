                  <span data-feather="calendar"></span>
                  This week
                </button>
              </div>
            </div>
  
            <div class="table-responsive">
              <table class="table table-striped table-sm">
                <thead>
                  <tr>
                    <th>Aadhar</th>
                    <th>Name</th>
                    <th>Mobile</th>
                    <th>Gender</th>
                    <th>District</th>
                    <th>State</th>
                    <th>Address</th>
                    <th class="text-center">Action</th>
                  </tr>
                </thead>
                <tbody>
                  <% docs.forEach(data => { %>
                    <% if(!data.isVerified) { %>
                        <tr>
                            <td><%= data.aadhar %></td>
                            <td><%= data.fullname %></td>
                            <td><%= data.mobile %></td>
                            <td><%= data.gender %></td>
                            <td><%= data.district %></td>
                            <td><%= data.state %></td>
                            <td><%= data.address %></td>
                            <td class="action">
                              <a class="verify btn btn-success text-white"  role="button" onclick="setVerified('<%= data._id %>')">Verify</a>
                              <a class="reject btn btn-danger text-white" role="button" onclick="setUnverified('<%= data._id %>')">Reject</a>
                            </td>
                          </tr>
                    <% } %>
                  <% }) %>
                </tbody>
              </table>
